   "source": [
    "d = [0]*100\n",
    "\n",
    "def fibonacci(n):\n",
    "    if n == 1 or n == 2:\n",
    "        return 1\n",
    "    \n",
    "    if d[n] != 0:\n",
    "        return d[n]\n",
    "    \n",
    "    d[n] = fibonacci(n-1)+fibonacci(n-2)\n",
    "    return d[n]\n",
    "\n",
    "print(fibonacci(99))"
   ]
